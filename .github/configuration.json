{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feat", "feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 📦 Uncategorized",
      "labels": []
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "label_extractor": [
    {
      "pattern": "(.*)/(.+)",
      "on_property": "branch",
      "target": "$1",
      "flags": "gu"
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": " - *#${{NUMBER}}* ${{TITLE}} [@${{AUTHOR}}]",
  "empty_template": "- no changes"
}
